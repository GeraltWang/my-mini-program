/**app.wxss**/
@import "weui.wxss";
@import 'static/icons/iconfont.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.page {
  width: 100vw;
  height: 100vh;
}

.map {
  width: 100%;
  height: 100%;
}

.iconsize {
  font-size: 24px;
}

.map-desc {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 70px;
  line-height: 35px;
  padding: 0 15px;
  background-color: #fff;
}